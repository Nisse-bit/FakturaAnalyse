        <Nullable>disable</Nullable>  <!-- Slå nullable warnings fra -->
        
        <ImplicitUsings>enable</ImplicitUsings>
        <UseWindowsForms>true</UseWindowsForms>
        
      <UserSecretsId>3f225f65-eb5a-493a-8172-2b0b306ff08a</UserSecretsId>
  </PropertyGroup>

    <ItemGroup>
        <PackageReference Include="Azure.AI.FormRecognizer" Version="4.1.0" />
